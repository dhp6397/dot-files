#define font1 GohuFont Nerd Font Mono 

! special
*.foreground:   #eff0eb
*.background:   #282a36 
*.cursorColor:  #97979B

! black
*.color0:       #282a36
*.color8:       #686868

! red
*.color1:       #FF5C57
*.color9:       #FF5C57

! green
*.color2:       #5AF78E
*.color10:      #5AF78E

! yellow
*.color3:       #F3F99D
*.color11:      #F3F99D

! blue
*.color4:       #57C7FF
*.color12:      #57C7FF

! magenta
*.color5:       #FF6AC1
*.color13:      #FF6AC1

! cyan
*.color6:       #9AEDFE
*.color14:      #9AEDFE

! white
*.color7:       #F1F1F0
*.color15:      #EFF0EB
 
Xft.dpi: 96

URxvt*letterSpace:    0
*font:                xft:GohuFont Nerd Font Mono:size=11
*boldFont:            xft:GohuFont Nerd Font Mono:bold:size=11
*italicFont:          xft:GohuFont Nerd Font Mono:italic:size=11
*boldItalicFont:      xft:GohuFont Nerd Font Mono:bold:italic:size=11
rofi.font:             GohuFont Nerd Font Mono 11
URxvt*scrollBar:                  false
URxvt*mouseWheelScrollPage:       false
URxvt*cursorBlink:                true
URxvt*saveLines:                  5000
URxvt.iso14755:                   false
URxvt.keysym.Shift-Control-V:     eval:paste_clipboard
URxvt.keysym.Shift-Control-C:     eval:selection_to_clipboard
URxvt*termName:                   rxvt-unicode-256color
URxvt*depth:                      24
URxvt*background:		  [90]#282a36
URxvt.internalBorder: 		  16
URxvt*skipBuiltinGlyphs:    	  true

